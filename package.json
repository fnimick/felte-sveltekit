{
  "name": "felte-sveltekit",
  "version": "0.0.1",
  "author": "Francis Nimick <fnimick@gmail.com>",
  "description": "A simple validation connector for Felte + Sveltekit Form Actions",
  "keywords": [
    "sveltekit",
    "svelte",
    "form",
    "validation",
    "zod",
    "forms",
    "felte"
  ],
  "license": "MIT",
  "repository": {
    "url": "https://github.com/fnimick/felte-sveltekit"
  },
  "scripts": {
    "dev": "vite dev",
    "build": "svelte-kit sync && svelte-package && publint",
    "prepublishOnly": "echo 'Did you mean to publish `./package/`, instead of `./`?' && exit 1",
    "test": "playwright test",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test:unit": "vitest",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write ."
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js"
    },
    "./client": {
      "types": "./dist/client/index.d.ts",
      "svelte": "./dist/client/index.js"
    },
    "./client/SuperDebug.svelte": {
      "types": "./dist/client/SuperDebug.svelte.d.ts",
      "svelte": "./dist/client/SuperDebug.svelte"
    },
    "./server": {
      "types": "./dist/server/index.d.ts",
      "svelte": "./dist/server/index.js"
    }
  },
  "typesVersions": {
    ">4.0": {
      "index": [
        "./dist/index.d.ts"
      ],
      "server": [
        "./dist/server/index.d.ts"
      ],
      "client": [
        "./dist/client/index.d.ts"
      ],
      "client/SuperDebug.svelte": [
        "./dist/client/SuperDebug.svelte.d.ts"
      ]
    }
  },
  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "peerDependencies": {
    "@felte/core": "^1.3.7",
    "@felte/validator-zod": "^1.0.13",
    "felte": "^1.2.7",
    "zod": "^3.21.4",
    "zod-form-data": "^2.0.0"
  },
  "devDependencies": {
    "@felte/core": "^1.3.7",
    "@felte/validator-zod": "^1.0.13",
    "@playwright/test": "^1.31.2",
    "@sveltejs/adapter-auto": "^1.0.3",
    "@sveltejs/kit": "^1.11.0",
    "@sveltejs/package": "^1.0.2",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "@typescript-eslint/parser": "^5.55.0",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "felte": "^1.2.7",
    "prettier": "^2.8.4",
    "prettier-plugin-svelte": "^2.9.0",
    "publint": "^0.1.10",
    "svelte": "^3.56.0",
    "svelte-check": "^2.10.3",
    "tslib": "^2.5.0",
    "typescript": "^4.9.5",
    "vite": "^4.1.4",
    "vitest": "^0.25.8",
    "zod": "^3.21.4",
    "zod-form-data": "^2.0.0"
  },
  "type": "module"
}
